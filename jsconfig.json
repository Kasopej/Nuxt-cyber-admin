{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "~/*": [
        "./*"
      ],
      "PageBases/*": [
        "./components/pages_base_definitions/*"
      ],
      "@/*": [
        "./*"
      ],
      "~~/*": [
        "./*"
      ],
      "@@/*": [
        "./*"
      ]
    },
    "types": [
      "@nuxt/types"
    ],
    "resolveJsonModule": true
  },
  "vueCompilerOptions": {
    "target": 2.7,
    // "target": 2, // For Vue version <= 2.6.14
  },
  "exclude": [
    "node_modules",
    ".nuxt",
    "dist"
  ]
}